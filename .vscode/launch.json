{
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "attach",
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}"
            // "cwd": "${workspaceRoot}/NodeJS"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Node.js Program",
            "program": "${file}",
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch angular",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/Angular/ngApp"
        }
    ]
}